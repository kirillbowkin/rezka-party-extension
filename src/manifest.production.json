{
    "manifest_version": 3,
    "name": "rezka-party",
    "description": "rezka party",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ]
}